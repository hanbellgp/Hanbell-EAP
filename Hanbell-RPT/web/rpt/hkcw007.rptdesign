            <method name="open"><![CDATA[importPackage(Packages.java.lang);


importPackage(Packages.cn.hanbell.efgp.rpt);

ejbClient = new HKCW007Report();
ejbClient.setEJB(params["JNDIName"]);

master = ejbClient.getEntity(params["formid"]);

count = 0;]]></method>
            <method name="fetch"><![CDATA[if(master==undefined || master==null){
	return false;
}

if(count < 1){
	dataSetRow["facno"] = master.getCompanyName();
	dataSetRow["applyDate"] = master.getApplyDate();
	dataSetRow["bankName"] = master.getBankName();	
	dataSetRow["applycompany"] = master.getApplycompany();
	dataSetRow["companyno"] = master.getCompanyno();	
	dataSetRow["applyUser"] = master.getUser().getUserName();
	dataSetRow["applyDept"] = master.getDept().getOrganizationUnitName();
	dataSetRow["applyType"] = master.getApplyType();
	dataSetRow["flowloan"] = master.getFlowloan();	
	dataSetRow["other1"] = master.getOther1();
	dataSetRow["projectname1"] = master.getProjectname1();
	dataSetRow["currency"] = master.getBibName();
	dataSetRow["interestRate"] = master.getInterestRate()/100;
	dataSetRow["total"] = master.getTotal();
	dataSetRow["date1"] = master.getDate1();
	dataSetRow["date2"] = master.getDate2();
	dataSetRow["capital"] = master.getCapital();
	dataSetRow["loanPurpose"] = master.getLoanPurpose();		
	count++
	return true;
}
return false;


]]></method>
            <method name="close"><![CDATA[ejbClient = null;]]></method>
